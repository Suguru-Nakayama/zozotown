.category_var
  .inner
    %ul.nest_list.clearfix
      %li.nest_path
        = link_to "ZOZOTOWN", ""
      %li.nest_icon
        %i.fas.fa-angle-right
      %li.nest_item
        %a ブランドから探す
.status_title
  .inner.clearfix
    .status_title_wrapper.clearfix
      %i.fas.fa-tags
      %h2 ブランド一覧
.body
  .inner
    %ul.tabs.clearfix
      %li
        = link_to "#", class: "tabs-btn" do
          %span すべて
      %li
        = link_to "#", class: "tabs-btn" do
          %span メンズ
      %li
        = link_to "#", class: "tabs-btn-ladies" do
          %span レディース
      %li
        = link_to "#", class: "tabs-btn" do
          %span キッズ
    .ctrl_bar
      .search_area
      .initials
        %ul.clearfix
          - ("A".."Z").each do |initial|
            %li
              %span
                = link_to initial, "#"
          %li
            = link_to "その他", "#"
    %dl.index_block
      %dt
